[begin] (FluxBox)
	[submenu] (Editors)
		[exec] (GVim) {gvim}
		[exec] (Vim) {xterm -e vim}
	[end]
	[submenu] (Graphics)
        [exec] (Gimp) {gimp}
		[exec] (Ristretto Image Viewer) {ristretto}
		[exec] (scanimage) {xterm -e scanimage}
	[end]
	[submenu] (Multimedia)
		[exec] (ALSA mixer) {xterm -e alsamixer}
		[exec] (Audacious) {audacious}
		[exec] (MusicBrainz Picard) {picard}
		[exec] (Spotify) {wine ~/.wine/drive_c/users/zzajac/Application\ Data/Spotify/spotify.exe}
		[exec] (VLC media player) {vlc}
	[end]
	[submenu] (Network)
		[exec] (Chromium) {chromium}
		[exec] (Google Music Manager) {google-musicmanager}
		[exec] (Pidgin Internet Messenger) {pidgin}
		[exec] (Skype) {skype}
		[exec] (Thunderbird) {thunderbird}
        [exec] (Liferea) {liferea}
		[exec] (TightVNC Viewer) {vncviewer}
        [exec] (Adobe Flash Player) {flash-player-properties}
	[end]
	[submenu] (Office)
        [exec] (Toggl Desktop) {/usr/bin/toggl-desktop}
		[exec] (Adobe Reader 9) {acroread}
		[exec] (LibreOffice Impress) {libreoffice}
		[exec] (PostgreSQL Database) {xterm -e psql template1}
		[exec] (Wunderlist) {wunderlist}
	[end]
	[submenu] (System)
        [exec] (1Password) {wine ~/.wine/drive_c/Program\ Files\ \(x86\)/1Password/1Password.exe}
        [exec] (KeePass) {keepass}
		[exec] (Terminal) {terminal}
		[exec] (Oracle VM VirtualBox) {VirtualBox}
		[submenu] (Settings)
            [exec] (AMD Catalyst Control Center [Administrative]) {amdxdg-su -c amdcccle}
            [exec] (Notifications) {xfce4-notifyd-config}
			[exec] (Manage Printing) {xdg-open http://localhost:631/}
			[exec] (Preferred Applications) {exo-preferred-applications}
			[exec] (Print Settings) {system-config-printer}
		[end]
	[end]
	[submenu] (Utilities)
		[exec] (7-Zip FM) {7zFM}
		[exec] (File Manager) {exo-open --launch FileManager}
		[exec] (HP Device Manager) {hp-toolbox}
		[exec] (Info) {xterm -e info}
	[end]
	[separator]
	[submenu] (FluxBox)
		[workspaces] (Workspaces)
		[submenu] (Styles)
			[stylesdir] (/usr/share/fluxbox/styles)
			[stylesdir] (~/.fluxbox/styles)
		[end]
		[config] (Configure)
		[reconfig] (Reconfig)
		[restart] (Restart)
		[separator]
		[exit] (Exit)
	[end]
[end]
